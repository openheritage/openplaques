<% @page_title = "New photo" %>
<h1><%= @page_title %></h1>

<%= form_for @photo do |f| %>
  <p><%= f.label :url %> <%= f.text_field :url %></p>
  <p><%= f.label :licence_id %> <%= f.collection_select :licence_id, @licences, :id, :name, {:include_blank => true} %></p>
  <br/>
  <p><%= f.label :photographer %> <%= f.text_field :photographer %></p>
  <p><%= f.label :photographer_url %> <%= f.text_field :photographer_url %></p>
  <p><%= f.label :file_url %> <%= f.text_field :file_url %></p>
  <p><%= f.label :plaque_id %> <%= f.text_field :plaque_id %></p>
  <p>This is a <%= f.collection_select :shot, Photo.shots, :to_s, :to_s, :include_blank => true %> shot</p>
    
  <%= f.submit "Create" %>
<% end %>