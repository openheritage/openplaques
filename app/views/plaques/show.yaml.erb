plaque<%= @plaque.id %>:
  inscription: "<%= @plaque.inscription %>"
<% if @plaque.location %>  location: "<%= @plaque.location.name %>"<% end %>
  erected_at: "<%= @plaque.erected_at_string %>"
<% if @plaque.organisations %>  organisations: "<%= @plaque.organisations.collect(:name).join(",") %>"<% end %>